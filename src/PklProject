amends "pkl:project"

local githubUser = "BenMMcLean"

package {
  name = "pkl-strings"
  baseUri = "package://github.com/\(githubUser)/\(name)" 
  version = read("file:.version").text.trim()
  packageZipUrl = "https://github.com/\(githubUser)/\(name)/releases/download/v\(version)/\(name)@\(version)"
}